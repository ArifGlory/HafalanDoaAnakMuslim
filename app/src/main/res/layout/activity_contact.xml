<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="glory.hafalandoaanakmuslim.ContactActivity"
    android:background="@drawable/back9"
    >


    <include
        android:id="@+id/toolbar"
        layout="@layout/layout_toolbar"/>
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Contact"
        android:textColor="#0B6121"
        android:id="@+id/txtjudulContact"
        android:layout_marginTop="50dp"
        android:layout_centerHorizontal="true"
        android:textSize="25sp"
        android:layout_below="@+id/toolbar"
        />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="Penjelasan Contact"
        android:textColor="#0B6121"
        android:id="@+id/txtpenjelContact"
        android:layout_marginTop="20dp"
        android:textSize="20sp"
        android:layout_below="@+id/txtjudulContact"
        />

</RelativeLayout>
